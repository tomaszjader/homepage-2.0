{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "i18n",
            "urls": [
                "assets/i18n/*.json"
            ],
            "cacheConfig": {
                "maxSize": 5,
                "maxAge": "1d",
                "strategy": "performance"
            }
        }
    ]
}